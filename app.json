{
  "expo": {
    "name": "LeoPays",
    "version": "0.0.0",
    "description": "LeoPays",
    "privacy": "unlisted",
    "slug": "leopays-app",
    "scheme": "leopays",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "contactUsEmail": "info@leopays.dev"
    }
  }
}